{
  "name": "@aura/db",
  "version": "0.16.0",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "dev": "pnpm watch",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "format": "biome format --write .",
    "format:check": "biome ci .",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "watch": "tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@aura/api-types": "workspace:^",
    "@aura/backend-common": "workspace:^",
    "@aura/config": "workspace:^",
    "@aura/constants": "workspace:^",
    "@aura/decorators": "workspace:^",
    "@aura/di": "workspace:^",
    "@aura/permissions": "workspace:^",
    "@aura/typeorm": "catalog:",
    "class-validator": "0.14.0",
    "flatted": "catalog:",
    "lodash": "catalog:",
    "aura-core": "workspace:^",
    "aura-workflow": "workspace:^",
    "nanoid": "catalog:",
    "p-lazy": "3.1.0",
    "reflect-metadata": "catalog:",
    "uuid": "catalog:",
    "xss": "catalog:"
  },
  "devDependencies": {
    "@aura/typescript-config": "workspace:*",
    "@types/lodash": "catalog:",
    "express": "5.1.0"
  }
}
